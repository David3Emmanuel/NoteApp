{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "notes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "note": {
            "type": "string"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "image": {
            "type": "string"
          },
          "id": {
            "type": "string"
          }
        },
        "required": ["title", "note", "date", "id"]
      }
    }
  },
  "required": ["notes"]
}